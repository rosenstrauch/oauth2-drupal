{
    "name": "chrishemmings/oauth2-drupal",
    "description": "Drupal OAuth2-Server Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "drupal"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Hemmings",
            "email": "chris@hemmin.gs"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisHemmings\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisHemmings\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
